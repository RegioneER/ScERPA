{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\%COMPUTERNAME%_log-.txt",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} ; {Level:u3} ; {Application} - {SourceContext} ; correlationID: {OperationGuid} ; status: {status} ; message: {Message} ; exception: {Exception}{NewLine}"

        }
      }
    ]
  },
  "Proxy": {
    "UseProxy": false,
    "Address": "",
    "UseDefaultCredentials": true
  },
  "Common": {
    "CurrentEnvironment": 1,
    "cdnRER": "https://localhost:7139/lib",
    "bootstrapitalia": "v2.16.1",
    "jquery": "v3.7.1",
    "echarts": "v6.0.0"
  },
  "ANPR": {
    "NumeroMassimoCodiciFiscali": 100
  },
  "Password": "{la tua password di cifratura dati su db}",
  "ConnectionStrings": {
    "ScERPAConnection": "{connessione al database sql server}"
  },
  "SuperAdminDevelop": {
    "Username": "Admin",
    "Cognome": "Super",
    "Nome": "Admin",
    "Email": "superadmin@example.com",
    "Password": "{la tua password sicura di superadmin di test}"
  }
}