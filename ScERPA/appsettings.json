{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"

      },
      {
        "Name": "File",
        "Args": {
          "path": "--logpath--\\%COMPUTERNAME%_log-.txt",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} ; {Level:u3} ; {Application} - {SourceContext} ; correlationID: {OperationGuid} ; status: {status} ; message: {Message} ; exception: {Exception}{NewLine}"

        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Proxy": {
    "UseProxy": false,
    "Address": "proxy:port",
    "UseDefaultCredentials": true
  },
  "Common": {
    "CurrentEnvironment": 2,
    "cdnRER": "{--url di cdn interna per library come bootstrapitalia, jquery etc---}",
    "bootstrapitalia": "v2.16.1",
    "bootstrapitaliaCss": "bootstrap-italia.min.css",
    "jquery": "v3.7.1",
    "echarts": "v5.5.0",
    "sperimental": true,
    "logoutUrl": "#",
    "accessibilityUrl": "#"
  },
  "ANPR": {
    "NumeroMassimoCodiciFiscali": 100
  },
  "WSO2": {
    "urlWhiteList": [ "{--base url di apigatway test--}", "{--base url di apigateway prod--}" ],
    "delay": 1000
  },
  "Kestrel": {
    "Limits": {
      "KeepAliveTimeout": "00:10:00",
      "RequestHeadersTimeout": "00:10:00"
    }
  },
  "corsOrigin": "*.localhost"
}